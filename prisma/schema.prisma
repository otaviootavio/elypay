generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x"]
}


datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model rooms {
  uuid      String   @id @unique @default(uuid())
  createdAt DateTime @default(now())
  users     String[]
}


model users {
  uuid      String   @id @unique @default(uuid())
  createdAt DateTime @default(now())
  name String
}
